{
	"name": "calico-colors",
	"description": "Calico Colors - A Webview View API Sample",
	"version": "0.0.1",
	"publisher": "vscode-samp2les",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"engines": {
		"vscode": "^1.50.0"
	},
	"extensionKind": [
		"ui",
		"workspace"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:calicoColors.colorsView",
		"onView:red",
		"onView:green",
		"onView:blue",
		"onCommand:calicoColors.addColor",
		"onCommand:calicoColors.clearColors"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "foo",
					"title": "Foo",
					"icon": "media/dep.svg"
				}
			]
		},
		"views": {
			"foo": [
				{
					"type": "webview",
					"id": "red",
					"name": "Red"
				},
				{
					"type": "webview",
					"id": "green",
					"name": "Green"
				},
				{
					"type": "webview",
					"id": "blue",
					"name": "Blue RCWH"
				},
				{
					"type": "tree",
					"id": "tree",
					"name": "Tree"
				}
				
			]
		},
		"commands": [
			{
				"command": "calicoColors.addColor",
				"category": "Calico Colors",
				"title": "Add Color"
			},
			{
				"command": "calicoColors.clearColors",
				"category": "Calico Colors",
				"title": "Clear Colors",
				"icon": "$(clear-all)"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "calicoColors.clearColors",
					"group": "navigation",
					"when": "view == calicoColors.colorsView"
				}
			]
		},
		"configuration":{
			"title": "Foo",
			"properties": {
				"webviewSlow": {"default": 0, "type": "number", "description": "Slow", "title": "Delay"}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -w -p ./"
	},
	"devDependencies": {
		"@types/vscode": "^1.50.0",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"eslint": "^8.26.0",
		"typescript": "^4.8.4"
	}
}
